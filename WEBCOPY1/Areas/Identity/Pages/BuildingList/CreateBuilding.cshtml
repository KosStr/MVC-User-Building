@page
@model WEBCOPY1.Areas.Identity.Pages.BuildingList.BuildingListModel

@{
    ViewData["Title"] = "CreateBuilding";
}

<h1>@ViewData["Title"]</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-route-returnUrl="@Model.ReturnUrl" method="post">
            <h4>Create a new account.</h4>
            <hr />
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Input.Street"></label>
                <input asp-for="Input.Street" class="form-control" />
                <span asp-validation-for="Input.Street" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Number"></label>
                <input asp-for="Input.Number" class="form-control" />
                <span asp-validation-for="Input.Number" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Square"></label>
                <input asp-for="Input.Square" class="form-control" />
                <span asp-validation-for="Input.Square" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Price"></label>
                <input asp-for="Input.Price" class="form-control" />
                <span asp-validation-for="Input.Price" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Input.Price"></label>
                <input asp-for="Input.Price" class="form-control" />
                <span asp-validation-for="Input.Price" class="text-danger"></span>
            </div>

            <button type="submit" class="btn btn-primary">Create</button>
        </form>
    </div>
</div>
